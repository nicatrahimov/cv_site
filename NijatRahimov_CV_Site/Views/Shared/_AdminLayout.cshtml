@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Start your development with a Dashboard for Bootstrap 4.">
    <meta name="author" content="Rahimov">
    
    <title>@ViewBag.Title - Nijat Rahimov</title>
    
    <link rel="icon" href="~/Images/icons8-admin-32.png" type="image/png">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
    <link rel="stylesheet" href="~/argon-dashboard-master/assets/vendor/nucleo.css" type="text/css">
    <link rel="stylesheet" href="~/argon-dashboard-master/assets/vendor/all.min.css" type="text/css">
    <link rel="stylesheet" href="~/argon-dashboard-master/assets/css/argon.css?v=1.2.0" type="text/css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">

</head>

<body>

@await Html.PartialAsync("/Views/Admin/PartialSideBar.cshtml")

<div class="main-content" id="panel">
    <div class="header bg-primary pb-6">
        <div class="container-fluid">
            <div class="header-body">
                <div class="row align-items-center py-4">
                    <div class="col-lg-6 col-7">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid mt--6">
        <div class="row">
            <div class="col">
                @RenderBody()
            </div>
        </div>
        <!-- Footer -->
        <footer class="footer pt-0">
            <div class="row align-items-center justify-content-lg-between">
                <div class="col-lg-6">
                    <div class="copyright text-center text-lg-left text-muted">
                        &copy; 2024 <a href="https://github.com/nicatrahimov" class="font-weight-bold ml-1" target="_blank">Nicat Rahimov</a>
                    </div>
                </div>
            </div>
        </footer>
    </div>
</div>

<script src="~/argon-dashboard-master/assets/vendor/jquery.min.js"></script>
<script src="~/argon-dashboard-master/assets/vendor/bootstrap.bundle.min.js"></script>
<script src="~/argon-dashboard-master/assets/vendor/js.cookie.js"></script>
<script src="~/argon-dashboard-master/assets/vendor/jquery.scrollbar.min.js"></script>
<script src="~/argon-dashboard-master/assets/vendor/jquery-scrollLock.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDTTfWur0PDbZWPr7Pmq8K3jiDp0_xUziI"></script>
<script src="~/argon-dashboard-master/assets/js/argon.js?v=1.2.0"></script>
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>

</html>
